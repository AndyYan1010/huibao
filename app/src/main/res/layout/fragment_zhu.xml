            android:id="@+id/tv_no"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:layout_weight="3"
            android:gravity="right|center_vertical"
            android:singleLine="true"
            android:textSize="18sp"/>
    </LinearLayout>

    <View
